    "--sjs-font-family": "'Geist', -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, sans-serif",
    "--sjs-font-size": "16px",
    "--sjs-primary-backcolor": "#2563eb",
    "--sjs-primary-backcolor-light": "rgba(37, 99, 235, 0.1)",
